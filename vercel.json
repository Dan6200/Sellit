{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm build",
  "rewrites": [
    { "source": "/(.*)", "destination": "/built/server/src/app.js" },
    {
      "source": "/.well-known/acme-challenge/(.*)",
      "destination": "/public/.well-known/acme-challenge/$1"
    }
  ]
}
